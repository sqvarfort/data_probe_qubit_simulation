file = {:home/nomis/Documents/UCL/Group Project/papers/nature07295.pdf:pdf},
isbn = {0028-0836},
issn = {0028-0836},
journal = {Nature},
keywords = {Manipulation,spin resonance},
number = {7216},
pages = {1085--1088},
pmid = {260252600039},
title = {{Solid-state quantum memory using the {\$}{\^{}}31{\$}P nuclear spin}},
volume = {455},
year = {2008}
}
@article{Morton2005,
abstract = {Systematic errors in spin rotation operations using simple rf pulses place severe limitations on the usefulness of the pulsed magnetic resonance methods in quantum computing applications. In particular, the fidelity of quantum logic operations performed on electron spin qubits falls well below the threshold for the application of quantum algorithms. Using three independent techniques, we demonstrate the use of composite pulses to improve this fidelity by several orders of magnitude. The observed high-fidelity operations are limited by pulse phase errors, but nevertheless fall within the limits required for the application of quantum error correction.},
