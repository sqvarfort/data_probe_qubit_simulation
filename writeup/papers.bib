author = {Muhonen, Juha T and Dehollain, Juan P and Laucht, Arne and Hudson, Fay E and Sekiguchi, Takeharu and Itoh, Kohei M and Jamieson, David N and McCallum, Jeffrey C. and Dzurak, Andrew S and Morello, Andrea},
