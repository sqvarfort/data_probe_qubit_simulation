isbn = {1476-4687 (Electronic) 0028-0836 (Linking)},
issn = {0028-0836},
journal = {Nature},
number = {7366},
pages = {574--578},
pmid = {21937989},
publisher = {Nature Publishing Group},
title = {{High-fidelity projective read-out of a solid-state spin quantum register}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21937989},
volume = {477},
year = {2011}
}
