@article{Alvarez2011,
abstract = {Decoherence is one of the most important obstacles that must be overcome in quantum information processing. It depends on the qubit-environment coupling strength, but also on the spectral composition of the noise generated by the environment. If the spectral density is known, fighting the effect of decoherence can be made more effective. Applying sequences of inversion pulses to the qubit system, we generate effective filter functions that probe the environmental spectral density. Comparing different pulse sequences, we recover the complete spectral density function and distinguish different contributions to the overall decoherence.},
archivePrefix = {arXiv},
arxivId = {1106.3463},
author = {{\'{A}}lvarez, Gonzalo A. and Suter, Dieter},
doi = {10.1103/PhysRevLett.107.230501},
